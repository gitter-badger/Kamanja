{
  "packagename": "com.ligadata.jtm.test.filter",
  "version": "0.0.1",
  "imports": [ "com.ligadata.Utils._" ],
  "transformation": [
    {
      "input": { "typename": "com.ligadata.kamanja.samples.messages.msg1" },

      "variables": [ { "name": "out3", "type": "System.Int" } ],

      "output": [{
                   "message": "com.ligadata.kamanja.test001.msg2",
                   "mapping": [ { "out1": "in1"}, {"out2": "in2"}, {"out4": "in3"} ],
                   "filter":  [ { "expression": "$score != -1 && $score < 100" }],
                   "compute": [ { "out3": "iplookup($in1).zipcode" } ]
                 },
                 {
                   "message": "com.ligadata.kamanja.test001.msg2",
                   "mapping": [ { "out2": "in1" } ]
                 }]
    },
    {
      "input": "com.ligadata.kamanja.samples.messages.msg1",
      "output": [{
                  "message": "com.ligadata.kamanja.test001.msg3",
                  "mapping": [ { "out1": "in1"} ]
                }]
    }
  ]
}
