{
  "Clusters": [
    {
      "ClusterId": "ligadata1",
      "DataStore": {"StoreType": "hashmap","SchemaName": "testdata","Location": "{InstallDirectory}/storage"},
      "StatusInfo": {
        "ClassName": "com.ligadata.keyvaluestore.HashMapAdapter$",
        "SchemaName": "statusinfo",
        "Location": "{InstallDirectory}/storage"
      },
      "ZooKeeperInfo": {
        "ZooKeeperNodeBasePath": "/kamanja",
        "ZooKeeperConnectString": "localhost:2181",
        "ZooKeeperSessionTimeoutMs": "30000",
        "ZooKeeperConnectionTimeoutMs": "30000"
      },
      "EnvironmentContext": {
        "classname": "com.ligadata.SimpleEnvContextImpl.SimpleEnvContextImpl$"
      },
      "Nodes": [
        {
          "NodeId": "1",
          "NodePort": 6541,
          "NodeIpAddr": "localhost",
          "JarPaths": [
            "{InstallDirectory}/lib/system",
            "{InstallDirectory}/lib/application"
          ],
          "Scala_home": "{ScalaInstallDirectory}",
          "Java_home": "{JavaInstallDirectory}",
          "Roles": [
            "RestAPI",
            "ProcessingEngine"
          ],
          "Classpath": ".:{InstallDirectory}/lib/system/Kamanja_2.10-1.3.0.jar"
        },
        {
          "NodeId": "2",
          "NodePort": 6542,
          "NodeIpAddr": "localhost",
          "JarPaths": [
            "{InstallDirectory}/lib/system",
            "{InstallDirectory}/lib/application"
          ],
          "Scala_home": "{ScalaInstallDirectory}",
          "Java_home": "{JavaInstallDirectory}",
          "Roles": [
            "RestAPI",
            "ProcessingEngine"
          ],
          "Classpath": ".:{InstallDirectory}/lib/system/Kamanja_2.10-1.3.0.jar"
        },
        {
          "NodeId": "3",
          "NodePort": 6543,
          "NodeIpAddr": "localhost",
          "JarPaths": [
            "{InstallDirectory}/lib/system",
            "{InstallDirectory}/lib/application"
          ],
          "Scala_home": "{ScalaInstallDirectory}",
          "Java_home": "{JavaInstallDirectory}",
          "Roles": [
            "RestAPI",
            "ProcessingEngine"
          ],
          "Classpath": ".:{InstallDirectory}/lib/system/Kamanja_2.10-1.3.0.jar"
        }
      ],
      "Adapters": [
        {
          "Name": "TestIn_1",
          "TypeString": "Input",
          "DataFormat": "CSV",
          "ClassName": "com.ligadata.InputAdapters.KafkaSimpleConsumer$",
          "AdapterSpecificCfg": {
            "HostList": "localhost:9092",
            "TopicName": "testin_1"
          }
        },
        {
          "Name": "TestOut_In_1",
          "TypeString": "Validate",
          "DataFormat": "JSON",
          "ClassName": "com.ligadata.InputAdapters.KafkaSimpleConsumer$",
          "AdapterSpecificCfg": {
            "HostList": "localhost:9092",
            "TopicName": "testout_1"
          }
        },
        {
          "Name": "TestOut_1",
          "TypeString": "Output",
          "InputAdapterToVerify": "TestOut_In_1",
          "ClassName": "com.ligadata.OutputAdapters.KafkaProducer$",
          "AdapterSpecificCfg": {
            "HostList": "localhost:9092",
            "TopicName": "testout_1"
          }
        },
        {
          "Name": "TestStatus_1",
          "TypeString": "Status",
          "ClassName": "com.ligadata.OutputAdapters.KafkaProducer$",
          "AdapterSpecificCfg": {
            "HostList": "localhost:9092",
            "TopicName": "teststatus_1"
          }
        }
      ]
    }
  ]
}
